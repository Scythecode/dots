{
  "layer": "top",
  "height": 34,
  "spacing": 1,
  "modules-left": [
    "hyprland/workspaces"
  ],
  "modules-center": [
    "memory",
    "cpu",
    "temperature"
  ],
  "modules-right": [
    "privacy",
    "network",
    "tray",
    "wireplumber",
    "clock"
  ],
  "clock": {
    "format-alt": "<b>{:%I:%M:%S}</b> ï€— ",
    "format": "<b>{:%A, %B %d, %Y %I:%M:%S}</b> ï€—",
    "interval": 1
  },
  "privacy": {
    "icon-spacing": 4,
    "icon-size": 16,
    "transition-duration": 250,
    "modules": [
      {
        "type": "screenshare",
        "tooltip": true,
        "tooltip-icon-size": 16
      },
      {
        "type": "audio-in",
        "tooltip": true,
        "tooltip-icon-size": 16
      }
    ]
  },
  "temperature": {
    "critical-threshold": 85,
    "format-critical": "ï‹‡ <b>{temperatureC}Â°C</b>",
